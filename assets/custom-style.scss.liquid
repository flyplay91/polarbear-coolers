@media only screen and (max-width: 1023px) and (min-width: 769px) {
  .header-middle .header-middle__inner .header-middle--search {
    flex-basis: 25%;
    max-width: 25%;
  }
}

@media only screen and (min-width: 769px) {
  .site-header {
    .mobile-nav-wrapper {
      top: auto;
      display: block;
    }
  }
}

@media only screen and (max-width: 768px) {
  /* header */
  #shopify-section-header {
    .site-header {
      position: static;
      border: none;
      .header-shop-category-dropdown {
        opacity: 1;
        top: auto;
        padding-left: 0;
        padding-right: 0;
        .header-shop-category-dropdown__inner {
          margin: 0;
          display: block;
          .shop-category-dropdown-left,
          .shop-category-dropdown-right {
            max-width: 100%;
            transform: scaleX(0);
            display: block;
            width: 0;
            float: left;
            transition: all 0.3s;
            &.active {
              transform: scaleX(1);
              width: 100%;
            }
          }
          .shop-category-dropdown-left {
            border: none;
            padding: 0;
            text-align: left;
            transform-origin: left;
            ul {
              li {
                border-top: 1px solid #d3d3d3;
                margin: 0;
                padding: 15px 20px;
                line-height: 30px;
                a:before {
                  display: none;
                }
                .sub-trigger {
                  display: block;
                  float: right;
                  .icon {
                    width: 30px;
                    height: 30px;
                    padding: 9px;
                  }
                }
              }
            }
          }
          .shop-category-dropdown-right {
            transform-origin: right;
            padding: 0;
            .back-main {
              display: block;
              border-top: 1px solid #d3d3d3;
              padding: 15px 20px;
              font-weight: 600;
              color: #6e6e6e;
              font-size: 15px;
              letter-spacing: 0.075em;
              line-height: 30px;
              .icon {
                width: 20px;
                height: 30px;
                padding: 9px 8px 9px 0;
              }
            }
            ul {
              li {
                margin: 0;
                padding: 15px 20px;
                border-top: 1px solid #d3d3d3;
                line-height: 30px;
              }
            }
          }
        }
      }
    }
  }
  .header-top {
    justify-content: flex-end;
    .header-top-left {
      display: none;
    }
    .header-top-right {
      .header-top-link {
        display: none;
      }
      span.header-top-link--phone {
        display: block;
        font-weight: 600;
        &:after {
          display: none;
        }
      }
    }
  }
  .header-middle {
    .header-middle__inner {
      margin: 0;
      .header-middle--toggle {
        display: block;
        order: 0;
        flex-basis: 12.5%;
        max-width: 50px;
        margin: 0 auto;
        text-align: center;
        .icon-close {
          width: 16px;
        }
      }
      .header-middle--category {
        display: none;
      }
      .header-middle--logo {
        order: 2;
        flex-basis: 50%;
        max-width: 50%;
      }
      .header-middle--search {
        order: 1;
        flex-basis: 12.5%;
        max-width: 12.5%;
        button {
          width: 100%;
          padding: 0;
          background: transparent;
          border: none;
          color: #ffffff;
          max-width: 50px;
          margin: 0 auto;
          &:hover {
            color: var(--color-text-focus);
          }
          span {
            display: none;
          }
        }
      }
      .header-middle--shop-sign-cart {
        order: 3;
        flex-basis: 25%;
        max-width: 25%;
        li {
          &.header-middle--shop,
          &.header-middle--sign {
            display: none;
          }
          &.header-middle--cart {
            flex-basis: auto;
            min-width: calc(50% + 20px);
            max-width: calc(50% + 20px);
            padding: 0;
            margin: 0 auto;
            background: #fcae46;
            justify-content: space-around;
            cursor: pointer;
            a {
              font-size: 0;
              .header-cart-count {
                padding: 2px 0;
              } 
            }
            svg {
              fill: #000000;
              margin: 0 5px;
            }
          }
        }
      }
    }
  }
  .header-bottom {
    /*position: relative;*/
    display: none;
    /* > li {
      > a {
        padding: 10px 10px 10px 16px;
        &:hover {
          font-weight: 100;
        }
      }
      &.has-dropdown:hover {
        .bottom-dropdown-menu {
          top: 100%;
          z-index: 20;
        }
      }
    } */
  }

  /* content */
  .page-container {
    .slideshow__text-wrap--mobile {
      display: none;
    }
    .index-section {
      padding-top: 20px;
      padding-bottom: 20px;
      &:last-child {
        padding-bottom: 20px;
      }
    }
  }
  .home-slideshow-block {
    .home-slideshow__controls {
      button {
        &.home-slideshow__arrow-previous {
          position: relative;
          float: left;
        }
        &.home-slideshow__arrow-next {
          position: relative;
          float: right;
        }
      }
    }
  }
  .home-image-text .feature-row {
    display: flex;
    .feature-row__text {
      padding-top: 0;
    }
    .feature-row__image-wrapper {
      margin: 0;
      .feature-row__image {
        position: relative;
        max-width: 500px !important;
      }
    }
  }

  /* footer */
  .page-container {
    .site-footer {
      margin-top: 0;
      padding: 25px 0;
      .footer-page-width {
        .footer-subscription-block {
          .input-group__field {
            margin: 0 0 3px;
            text-align: center;
          }
          .input-group__hint {
            display: none;
          }
        }
        .footer-logo-links {
          flex-direction: column;
          padding-top: 12px;
          .footer-logo-block {
            flex-basis: auto;
            max-width: 100%;
            padding: 0;
            overflow: hidden;
            > a {
              display: none;
            }
            .h4 {
              display: block;
            }
            label {
              display: none;
            }
            .footer-social-block {
              margin-bottom: 20px;
            }
          }
          .footer-links-block {
            flex-basis: 100%;
            max-width: 100%;
            margin: 0;
            .footer-links-item {
              flex-basis: 100%;
              max-width: 100%;
              overflow: hidden;
              p.h4 {
                margin-bottom: 12.5px;
              }
              .site-footer__linklist li:last-child {
                margin-bottom: 20px;
              }
            }

          }
          p.h4 {
            font-size: 12px;
            color: white;
            text-transform: uppercase;
            border-bottom: 1px solid #fff;
            padding: 10px 0;
            font-weight: 600;
            &:after {
              content: '+';
              font-size: 18px;
              font-weight: 400;
              float: right;
              margin-top: -3px;
            }
          }
          .site-footer__sublist,
          .site-footer__linklist {
            transition: all 0.3s;
            transform: scaleY(0);
            transform-origin: top;
            max-height: 0;
            opacity: 0;
          }
          .active {
            p.h4:after {
              content: '-';
            }
            .site-footer__sublist,
            .site-footer__linklist {
              transform: scaleY(1);
              max-height: 1000px;
              height: auto;
              transition: all 0.3s;
              opacity: 1;
            }
          }
        }
      }
    }
  }
}


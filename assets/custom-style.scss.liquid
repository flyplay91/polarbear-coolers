html {
	
}
h1,h2,h3,h4,h5,h6,span,p,label,a {

  color: #4a4a4a;
}
button:focus {
  outline: none;
}
#RecoverPassword {color: #0f0f0f;}


.header-top {
	display: flex;
  align-items: center;
  justify-content: space-between;
  max-width: 100%;
  margin: auto;
  font-size: 12px;
  border-bottom: 1px solid rgba(205, 205, 205, 0.7);
  background-color: #000;

  .header-top-right {
  	display: flex;

  	span.header-top-link {
  		padding: 6px 16px;
	    position: relative;
	    font-weight: 300;
	    color: white;
	    font-size: 12px;
	    line-height: 1.8;
	    letter-spacing: 1.2px;
	    text-transform: uppercase;

	    &:after {
	    	width: 1px;
		    height: 18px;
		    position: absolute;
		    content: '';
		    background-color: rgba(205, 205, 205, 0.7);
		    opacity: 0.75;
		    -webkit-transform: translateX(1rem);
		    -ms-transform: translateX(1rem);
		    transform: translateX(1rem);
	    }
		}
    
		a.header-top-link {
  		padding: 6px 16px;
	    position: relative;
	    font-weight: 300;
	    color: #ff830f;
	    font-size: 12px;
	    line-height: 1.8;
	    letter-spacing: 1.2px;
	    text-transform: uppercase;

	    &:after {
	    	width: 1px;
		    height: 18px;
		    position: absolute;
		    content: '';
		    background-color: rgba(205, 205, 205, 0.7);
		    opacity: 0.75;
		    -webkit-transform: translateX(1rem);
		    -ms-transform: translateX(1rem);
		    transform: translateX(1rem);
	    }
          
          &:hover {color:#ffffff;}
          
          
		}
  }
}

.header-middle {
	max-width: 100%;
  padding: 16px;
  position: relative;
  background-color: #353537;
  color: #ffffff;

  .header-middle__inner {
  	display: flex;
    flex: 0 1 auto;
    flex-direction: row;
    flex-wrap: wrap;
    padding: 0;
    list-style: none;
    transition: all .3s ease;
    margin: 0 0 0 calc(0px - 16px);
    align-items: center;

    .header-middle--logo {
    	flex-basis: 16.666%;
    	max-width: 16.666%;
    	font-size: 0;
    	flex: 1 1 auto;
    	padding-left: 16px;

    	> a > img {
    		width: 100%;
    	}
    }

    .header-middle--category {
    	flex-basis: 16.666%;
    	max-width: 16.666%;
    	flex: 1 1 auto;
    	padding-left: 16px;

    	button {
		    margin: 0;
		    padding: calc(0.5em - 1px) calc(1em - 1px);
		    font-size: 16px;
		    font-weight: 600;
		    letter-spacing: 0.025em;
        background-color: transparent;
        border: none;
        -webkit-appearance: none;
        appearance: none;
        cursor: pointer;
        transition: color 0.3s ease, background-color 0.3s ease, border-color 0.3s ease, box-shadow 0.3s ease;
        -webkit-user-select: none;
        -moz-user-select: none;
        -ms-user-select: none;
        user-select: none;
        letter-spacing: 1px;
        &:hover {
          color: #a1a1a1;
        }

        img {
          width: 16px;
        }
    	}
    }

    .header-middle--search {
      flex-basis: 25%;
      max-width: 25%;
      text-align: center;
      padding-left: 16px;
      button {
        margin: 0;
        position: relative;
        vertical-align: middle;
        font-family: inherit;
        font-size: 16px;
        font-weight: 400;
        text-shadow: none;
        letter-spacing: 0.05em;
        line-height: 1.5;
        text-align: center;
        text-decoration: none;
        background-color: white;
        border: 1px solid #b3b3b3;
        border-radius: 0;
        appearance: none;
        cursor: pointer;
        color: #6e6e6e;
        display: block;
        width: 100%;
        padding: 15px 30px;
      }
    }

    .header-middle--shop-sign-cart {
      display: flex;
      margin-bottom: 0;
      flex-basis: 41.666%;
      max-width: 41.666%;
      text-align: center;
      flex: 1 1 auto;
      padding-left: 16px;

      li {
        flex-basis: calc(100% / 3);
        max-width: calc(100% / 3);
        display: block;
        flex: 1 1 auto;
        padding-left: 16px;

        a {
          color: white;
          font-weight: 600;
          letter-spacing: 0.025em;
          transition: 0.2s;

          &:hover {
            color: #a1a1a1;
          }

          .header-cart-count {
            margin: 0 4px;
            padding: 5px 9px;
            font-size: 14px;
            font-weight: 600;
            letter-spacing: 0.4px;
            line-height: 25.2px;
            background-color: #fcae46;
            color: #333;
          }
        }

        &.header-middle--sign a img {
          width: 12px;
        }

        &.header-middle--cart img {
          width: 16px;
          cursor: pointer;
        }
      }
    }
  }
}

.header-bottom {
  display: flex;
  flex-wrap: wrap;
  width: 100%;

  > li {
    flex-grow: 1;
    list-style-type: none;
    margin: 0;
    flex-basis: auto;
    display: flex;
    flex-direction: column;

    > a {
      background-color: #f58220;
      color: #fff;
      border-right: 1px solid #000;
      box-shadow: inset 0 1px rgba(255,255,255,.18), inset 0 20px rgba(255,255,255,.1), inset 0 30px 20px rgba(255,255,255,.1);
      display: block;
      text-decoration: none;
      white-space: nowrap;
      transition: all linear .3s;
      padding: 10px 6px 10px 10px;
      text-align: center;
      letter-spacing: 0.05em;
      font-size: 16px;
      z-index: 1;
      font-weight: 100;
      &:hover {
        background-color: rgba(134,134,134,.9);
        color: #fff;
        font-weight: 600;
      }
    }

    &.has-dropdown {
      > a:before {
        content: "\25BA";
        margin-left: -1.1em;
        padding-right: .2em;
        font-weight: 700;
        font-size: 70%;
        position: relative;
        top: -.2em;
      }

      &:hover {
          
        .bottom-dropdown-menu {
          transition: transform 800ms ease 0s, opacity 800ms ease 0s;
          transform: scale(1, 1);
          opacity: 1;
        }
      }
    }

    .bottom-dropdown-menu {
      max-width: 90%;
      position: absolute;
      top: 190px;
      left: 0;
      right: 0;
      margin: auto;
      width: 100%;
      clip: auto;
      visibility: visible;
      height: auto;
      overflow: visible;
      padding: 5px;
      border-radius: 5px;
      background-color: black;
      z-index: 1;
      transition: transform 800ms ease 0s, opacity 800ms ease 0s;
      transform: scale(0, 1);

      &.accessories-dropdown {
        max-width: 12%;
        left: auto;
        background-color: #868686e6;
        padding: 10px;
        ul li {
          &:first-child {
            border-bottom: solid 1px black;
          }
          a {
            font-weight: 500;
          }
        }
      }


      .bottom-dropdown-menu-wrapper {
        .bottom-dropdown-menu-heading {
          margin: 5px;
          box-sizing: border-box;
          padding: 40px 20px 20px;
          text-align: center;

          h3 {
            margin: 0;
            font-weight: 900;
            font-size: 20.8px;
            color:#fff;
          }
        }

        .bottom-dropdown-menu-body {
          display: flex;

          .d-w-50 {
            width: 50%;
          }

          .d-w-25 {
            width: 25%;
          }

          .d-w-22 {
            width: 22%;
          }

          .d-w-34 {
            width: 34%;
          }

          .bottom-dropdown-menu-item {
            
            background-color: #fff;
            color: #000;
            margin: 5px;
            box-sizing: border-box;
            border-radius: 5px;
            padding: 20px;

            h4 {
              font-size: 19.2px;
               font-weight: 900;
            }

            p {
              line-height: 1.6;
              font-size: 18px;
            }

            a {
              background-color: #fcae46;
              display: block;
              text-align: center;
              text-decoration: none;
              max-width: 250px;
              margin: 0 auto;
              font-size: 20px;
              font-weight: 700;
              padding: 10px 0;
              &:hover {
                background-color: #e17824;
              }
            }
          }
        }

        .bottom-dropdown-menu-bottom {
          padding: 20px;
          a {
            letter-spacing: 0.8px;
            background-color: #fcae46;
            color: white;
            display: block;
            text-decoration: none;
            max-width: 400px;
            margin: 0 auto;
            font-weight: 700;
            font-size: 20px;
            padding: calc(0.5em * 3 - 1px) 0;
            text-align: center;
            text-transform: uppercase;

            &:hover {
              background-color: #e17824;
            }
          }
        }
      }
    }
  }
}

.header-shop-category-dropdown {
  padding: 56px 20px;
  width: 100%;
  min-height: 100%;
  position: absolute;
  background-color: #f7f7f7;
  opacity: 0;
  overflow-x: hidden;
  transition: all ease-in-out 0.15s;
  -webkit-transform: scaleY(0);
  -ms-transform: scaleY(0);
  transform: scaleY(0);
  transform-origin: top;
  z-index: 300;

  &.is-open {
    opacity: 1;
    -webkit-transform: scaleY(1);
    -ms-transform: scaleY(1);
    transform: scaleY(1);
  }

  a {
    color: #6e6e6e;
    font-weight: 600;

    &:hover {
      color: #333;
    }
  }

  .header-shop-category-dropdown__inner {
    display: flex;
    flex: 0 1 auto;
    flex-direction: row;
    flex-wrap: wrap;
    margin: 0 0 0 calc(0px - 16px);
    padding: 0;
    list-style: none;
    transition: all .3s ease;

    .shop-category-dropdown-left {
      flex-basis: 25%;
      max-width: 25%;
      height: 100%;
      padding-right: 4.5rem;
      text-align: right;
      border-right: 2px solid #d8d8d8;
      overflow: auto;
      overflow-y: overlay;

      ul {
        
        li {
          margin-bottom: 0.75em;
          
          a {
            display: inline-block;
            font-weight: 600;
            letter-spacing: 0.075em;
            text-decoration: none;
            color: #6e6e6e;
            white-space: nowrap;

            &:hover {
              color: #333;

              &:before {
                -webkit-transform: scaleX(1) translateY(calc(-1em / 2 + 3px));
                -ms-transform: scaleX(1) translateY(calc(-1em / 2 + 3px));
                transform: scaleX(1) translateY(calc(-1em / 2 + 3px));
              }
            }

            &:before {
              display: inline-block;
              width: 1.25em;
              height: 2px;
              margin-right: 0.375em;
              background-color: currentColor;
              content: '';
              -webkit-transform: scaleX(0) translateY(calc(-1em / 2 + 3px));
              -ms-transform: scaleX(0) translateY(calc(-1em / 2 + 3px));
              transform: scaleX(0) translateY(calc(-1em / 2 + 3px));
              transform-origin: right;
              transition: all linear 0.1s;
            }
          }
        }
      }
    }

    .shop-category-dropdown-right {
      height: 100%;
      padding-left: 6rem;
      flex-basis: 75%;
      max-width: 75%;

      ul {
        
        li {
          margin-bottom: 0.75em;
          
          a {
            display: inline-block;
            font-weight: 600;
            letter-spacing: 0.075em;
            text-decoration: none;
            color: #6e6e6e;
            white-space: nowrap;

            &:hover {
              color: #333;
            }
          }
        }
      }
    }
  }
}

.category-dropdown-right-item {
  display: none;

  &.active {
    display: block;
  }
}

.header-login-form {
  width: 360px;
  position: absolute;
  right: 0;
  z-index: 111;
  background-color: #f7f7f7;
  border: 1px solid rgba(205, 205, 205, 0.7);
  padding: 1rem;
  right: 0;
  border: 1px solid rgba(205, 205, 205, 0.7);
  box-shadow: 0 2px 1px -2px rgba(205, 205, 205, 0.7);
   opacity: 0; 
  transition: all ease-in-out 0.15s;
  -webkit-transform: scaleY(0); 
  transform: scaleY(0); 
  transform-origin: top;

  &.active {
    opacity: 1;
    -webkit-transform: scaleY(1);
    transform: scaleY(1);
  }

  .header-signin-btn {
    width: 100% !important;
    padding: calc(0.5em * 3 - 1px) calc(1em * 3 - 1px);
  }

  #customer_register_link {
    background-color: #f7f7f7;
    color: #333;
    text-transform: uppercase;
    font-weight: 600;
    font-size: 0.832em;
    border: solid 1px rgb(51, 51, 51);
    padding: calc(0.5em * 3 - 1px) calc(1em * 3 - 1px);
    width: 100%;
    display: block;

    &:hover {
      background-color: #bcbcbc;
      color: #ccc;
    }
  }

  #RecoverPassword {
    color: #333;
    text-align: center;
    display: block;
    font-size: 12px;
    padding: 10px 0;
    font-weight: 500;
    text-transform: uppercase;
  }
}


/* ===== Homepage Slideshow ====== */
.home-slideshow-block {
  .home-slideshow__controls {
    position: absolute;
    top: 50%;
    transform: translateY(-50%);
    z-index: 11;
    width: 100%;

    button {
      background-color: transparent;
      border: none;
      text-shadow: 1px 1px 10px #000;

      img {
        width: 27px;
      }

      &.home-slideshow__arrow-previous {
        position: absolute;
        left: 30px;
        width: 30px;
        padding: 0;
        cursor: pointer;
      }

      &.home-slideshow__arrow-next {
        position: absolute;
        right: 30px;
        width: 30px;
        padding: 0;
        cursor: pointer;
      }
    }
  }
}
/* ===== End HomePage Slideshow ====== */

/* ===== Homepage Image With Text ====== */

.home-image-text {
  .feature-row {
    display: grid;
    grid-template-columns: repeat(2, 1fr);
  }  
  .feature-row__item {
    padding-right: .75rem;
    padding-left: .75rem;
    text-align: center;

    &.feature-row__text {
      img {
        width: 100%;
        max-width: 375px;
      }

      h2.h3 {
        margin-bottom: 1.125rem;
        font-size: 24px;
        font-weight: 400;
        text-align: center;
        color: #4a4a4a;
        line-height: 1.8;
      }

      .rte {
        p {
          text-align: center;
          font-size: 18px;
          font-weight: 300;
          line-height: 1.444;
        }
      }

      a.btn {
        background-color: #fcae46;
        display: block;
        max-width: 180px;
        font-weight: 700;
        font-size: 1.25em;
        margin: 0 auto;

        &:hover {
          background-color: #e17824;
        }
      }
    }
  }
}

/* ===== Footer Style ====== */
.site-footer {
  .footer-page-width {
    max-width: 1760px;
    padding: 20px 16px 0 16px;
    margin: 0 auto;

    .footer-subscription-block {
      max-width: 600px;
      margin: 0 auto;
    }

    .footer-logo-links {
      display: flex;
      flex: 0 1 auto;
      flex-direction: row;
      flex-wrap: wrap;
      padding: 32px 0;

      .footer-logo-block {
        flex-basis: 33.333%;
        max-width: 33.333%;

        .footer-time {
          margin-top: 10px;
          label {
            margin-bottom: 3px;
            color: #6e6e6e;
            text-transform: uppercase;
            font-weight: 600;
            font-size: 14px;
          }

          span {
            text-transform: uppercase;
            font-weight: 600;
            font-size: 16px;
          }
        }

        .footer-social-block {
          margin-top: 10px;
          label {
            margin-bottom: 3px;
            color: #6e6e6e;
            text-transform: uppercase;
            font-weight: 600;
            font-size: 14px;
          }

          ul.footer-social {
            display: flex;
            li {
              margin-right: 16px;
            }
          }
        }
      }

      .footer-links-block {
        flex-basis: 66.666%;
        max-width: 66.666%;
        display: flex;
        flex: 1 1 auto;
        flex-direction: row;
        flex-wrap: wrap;
        margin: 0 0 0 calc(0px - 16px);

        .footer-links-item {
          flex-basis: calc(100% / 3);
          max-width: calc(100% / 3);
          flex: 1 1 auto;

          p.h4 {
            margin-bottom: 3px;
            font-weight: 600;
            text-transform: uppercase;
            font-size: 12px;
          }

          .site-footer__linklist {
            li {
              padding: 0 30px 5px 0;

              a {
                font-size: 16px;
                font-weight: 600;
                line-height: 28px;
                letter-spacing: 0.4px;
              }
            }
          }


        }
      }
    }
  }
}


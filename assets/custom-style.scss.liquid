      border: 1px solid #d4cccc;
      padding: 5px;
      margin: 10px 0;
      cursor: pointer;
      @media(max-width: 600px) {
        margin: 10px;
      }
    }

    .slick-arrow {
      position: absolute;
      height: 32px;
      left: 50%;
      transform: translateX(-50%);
      opacity: 0.75;
      background-position: center;
      z-index: 11;

      &:before {
        content: none;
      }

      &.slick-prev {
        background-image: url({{'up-arrow.svg' | asset_url}});
        background-repeat: no-repeat;
        top: -25px;

        @media(max-width: 600px) {
          left: 0;
          top: 50%;
          transform: translateY(-50%) rotate(-90deg);
        }
      }

      &.slick-next {
        background-image: url({{'down-arrow.svg' | asset_url}});
        background-repeat: no-repeat;
        bottom: -25px;
        right: auto;
        top: auto;

        @media(max-width: 600px) {
          right: 0;
          top: 50%;
          transform: translateY(-50%) rotate(-90deg);
          bottom: auto;
          left: auto;
        }
      }
    }
  }
}


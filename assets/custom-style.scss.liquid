.header-top {
	display: flex;
  align-items: center;
  justify-content: space-between;
  max-width: 100%;
  margin: auto;
  font-size: 12px;
  border-bottom: 1px solid rgba(205, 205, 205, 0.7);
  background-color: #000;

  .header-top-right {
  	display: flex;

  	span.header-top-link {
  		padding: 6px 16px;
	    position: relative;
	    font-weight: 300;
	    color: white;
	    font-size: 12px;
	    line-height: 1.8;
	    letter-spacing: 1.2px;
	    text-transform: uppercase;

	    &:after {
	    	width: 1px;
		    height: 18px;
		    position: absolute;
		    content: '';
		    background-color: rgba(205, 205, 205, 0.7);
		    opacity: 0.75;
		    -webkit-transform: translateX(1rem);
		    -ms-transform: translateX(1rem);
		    transform: translateX(1rem);
	    }
		}
    
		a.header-top-link {
  		padding: 6px 16px;
	    position: relative;
	    font-weight: 300;
	    color: #ff830f;
	    font-size: 12px;
	    line-height: 1.8;
	    letter-spacing: 1.2px;
	    text-transform: uppercase;

	    &:after {
	    	width: 1px;
		    height: 18px;
		    position: absolute;
		    content: '';
		    background-color: rgba(205, 205, 205, 0.7);
		    opacity: 0.75;
		    -webkit-transform: translateX(1rem);
		    -ms-transform: translateX(1rem);
		    transform: translateX(1rem);
	    }
          
          &:hover {color:#ffffff;}
          
          
		}
  }
}

.close-popup > a {

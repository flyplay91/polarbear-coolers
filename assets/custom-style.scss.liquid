    order: 2;
    border: 1px solid #d4cccc;
    opacity: 0;
    visibility: hidden;
    padding: 30px;

    @media(max-width: 600px) {
      width: 100%;
      order: 1;
    }
  }

  .product-thumb-images {
    order: 1;
    opacity: 0;
    visibility: hidden;
    width: 90px;
    margin-right: 15px;
    position: relative;
    
    @media(max-width: 600px) {
      order: 2;
      width: 100%;
      margin-right: 0;
      display: block;
      padding: 0 45px;
    }

